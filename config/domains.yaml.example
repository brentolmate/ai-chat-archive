    - essay
    - copy
    - content writing
    - editing

# Example: Development domain
development:
  keywords:
    - programming
    - software
    - app
    - web dev
    - frontend
    - backend
    - database

# Example: Health domain
health:
  keywords:
    - fitness
    - workout
    - nutrition
    - meditation
    - sleep
    - wellness
